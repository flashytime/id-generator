{
    "name": "flashytime/id-generator",
    "description": "ID Generator based on Apc and MySQL",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "flashytime",
            "email": "myflashytime@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-apcu": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5"
    },
    "autoload": {
        "psr-4": {
            "Flashytime\\IdGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flashytime\\IdGenerator\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
